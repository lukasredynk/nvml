POOL Header:
Signature                : PMEMBLK
Major                    : $(*)
Mandatory features       : $(*)
Not mandatory features   : $(*)
Forced RO                : $(*)
UUID                     : $(*)
Creation Time            : $(*)
Checksum                 : $(*) [OK]

PMEM BLK Header:
Block size               : 512

[ARENA 0]
PMEM BLK BTT Info Header:
Signature                : BTT_ARENA_INFO
UUID of container        : $(*)
Flags                    : $(*)
Major                    : $(*)
Minor                    : $(*)
External LBA size        : $(*)
External LBA count       : $(*)
Internal LBA size        : $(*)
Internal LBA count       : $(*)
Free blocks              : $(*)
Info block size          : $(*)
Next arena offset        : $(*)
Arena data offset        : $(*)
Area map offset          : $(*)
Area flog offset         : $(*)
Info block backup offset : $(*)
Checksum                 : $(*) [OK]

PMEM BLK BTT Info Header Backup:
Signature                : BTT_ARENA_INFO
UUID of container        : $(*)
Flags                    : $(*)
Major                    : $(*)
Minor                    : $(*)
External LBA size        : $(*)
External LBA count       : $(*)
Internal LBA size        : $(*)
Internal LBA count       : $(*)
Free blocks              : $(*)
Info block size          : $(*)
Next arena offset        : $(*)
Arena data offset        : $(*)
Area map offset          : $(*)
Area flog offset         : $(*)
Info block backup offset : $(*)
Checksum                 : $(*) [OK]
