POOL Header:
Signature                : PMEMBLK
Major                    : $(*)
Mandatory features       : $(*)
Not mandatory features   : $(*)
Forced RO                : $(*)
UUID                     : $(*)
Creation Time            : $(*)
Checksum                 : $(*) [OK]

PMEM BLK Header:
Block size               : $(*)

[ARENA 0]
PMEM BLK BTT Info Header:
Signature                : BTT_ARENA_INFO
UUID of container        : $(*)
Flags                    : $(*)
Major                    : $(*)
Minor                    : $(*)
External LBA size        : $(*)
External LBA count       : $(*)
Internal LBA size        : $(*)
Internal LBA count       : $(*)
Free blocks              : $(*)
Info block size          : $(*)
Next arena offset        : $(*)
Arena data offset        : $(*)
Area map offset          : $(*)
Area flog offset         : $(*)
Info block backup offset : $(*)
Checksum                 : $(*) [OK]

PMEM BLK Statistics:
Total blocks             : $(*)
Zeroed blocks            : $(*) [100 %]
Error blocks             : 0 [0 %]
Blocks without flag      : 0 [0 %]

POOL Header:
Signature                : PMEMLOG
Major                    : $(*)
Mandatory features       : $(*)
Not mandatory features   : $(*)
Forced RO                : $(*)
UUID                     : $(*)
Creation Time            : $(*)
Checksum                 : $(*) [OK]

PMEM LOG header:
Start offset             : $(*)
Write offset             : $(*) [OK]
End offset               : $(*)

PMEM LOG statistics:
Total                    : $(*)
Available                : $(*) [100 %]
Used                     : 0 [0 %]
